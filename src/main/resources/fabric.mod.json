{
    "schemaVersion": 1,
    "id": "jamboree",
    "version": "${version}",

    "environment": "client",
    "entrypoints": {
        "main": [
            {
                "value": "uk.mrjamesco.jamboree.Jamboree",
                "adapter": "kotlin"
            }
        ],
        "modmenu": [
            {
                "value": "uk.mrjamesco.jamboree.ModMenuIntegration",
                "adapter": "kotlin"
            }
        ]
    },
    "mixins": [
        "jamboree.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "minecraft": "~${minecraft_version}",
        "java": ">=21",
        "fabric-api": "*",
        "fabric-language-kotlin": "*",
        "yet_another_config_lib_v3": ">=${yacl_version}"
    },
    "recommends": {
        "noxesium": ">=${noxesium_version}"
    },

    "name": "Jamboree",
    "description": "Miscellaneous utility stuff (including for MCC Island)",
    "contact": {
        "homepage": "https://github.com/JamesMCo/jamboree",
        "issues": "https://github.com/JamesMCo/jamboree/issues",
        "sources": "https://github.com/JamesMCo/jamboree"
    },
    "authors": [
        {
            "name": "James C.",
            "contact": {
                "homepage": "https://mrjamesco.uk"
            }
        }
    ],
    "license": "MIT",
    "icon": "assets/jamboree/icon.png"
}
